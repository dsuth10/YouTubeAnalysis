# YouTube Data API Key (Get from Google Cloud Console)
YOUTUBE_API_KEY=your_youtube_api_key_here

# OpenRouter API Key (Get from https://openrouter.ai/)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Apify API Key for transcript fallback
APIFY_API_KEY=your_apify_api_key_here

# Notion API Integration Token (Get from https://www.notion.so/my-integrations)
NOTION_TOKEN=your_notion_integration_token_here

# Notion Database ID (Optional - can be set via UI)
NOTION_DATABASE_ID=your_notion_database_id_here

# Server Configuration
PORT=3000 